{
  "name": "Tab Terminator",
  "version": "1.0",
  "description": "Simple extension that closes tab after a specific period of inactivity.",
  "permissions": ["storage", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_page": "index.html",
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "manifest_version": 2
}